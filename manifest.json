{
  "manifest_version": 2,
  "name": "YouTube Ad Skipper Extension",
  "short_name": "Ad Skipper",
  "version": "1.0.0",
  "description": "This is an extension to skip annoying YouTube Ads.",
  "author": "Hazem Gharib",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Skip annoying YouTube Ads!"
  }
}
